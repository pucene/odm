{
    "name": "pucene/odm",
    "description": "Object-Document Mapping for pucene",
    "authors": [
        {
            "name": "Pucene Community",
            "homepage": "https://github.com/pucene/odm/contributors"
        }
    ],
    "keywords": [
        "lucene",
        "indexing"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "psr/simple-cache": "^1.0",
        "doctrine/annotations": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pucene\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pucene\\Tests\\": "tests/src/"
        }
    }
}
